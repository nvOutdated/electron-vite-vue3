{
  "name": "my-new-app",
  "productName": "my-new-app",
  "version": "1.0.0",
  "description": "My Electron application with Vue 3 and Pinia",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "dev": "vite",
    "dev:electron": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "vite build",
    "preview": "vite preview",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx,.vue --ignore-path .gitignore .",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "electron",
    "vue",
    "pinia",
    "vite"
  ],
  "author": {
    "name": "xdd",
    "email": "1213792208@qq.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "38.1.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.0.0",
    "typescript": "~5.0.0",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.11.0",
    "vite-plugin-electron-renderer": "^0.14.0",
    "vue-tsc": "^1.8.0",
    "wait-on": "^8.0.5"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "element-plus": "^2.11.2",
    "pinia": "^2.1.0",
    "vue": "^3.3.0"
  }
}
